<!doctype html>
<html>
  <head>
    <title>Welcome to <%=gymName%></title>
    <link rel="stylesheet"  type="text/css" href="main.css"/>
  </head>
  <body>
    <div class="menu">
      <button class="menu_button">Menu</button>
      <div class="menu-content">
        <a href="/">Home</a>
        <a href="about">About Us</a>
        <a href="weather">Check the weather!</a>   
        <a href="classes">See whats on this week</a>
        <a href="search">Find a class</a>
        <a href="bookings">Book a class</a>  
        <a href="profile">Manage account</a>
        <a href="logout">Logout</a>
      </div>
    </div>
    <h1>What's on at <%=gymName%></h1>
    <div class="table_box">
      <table>
        <tr>
          <th>Name</th>
          <th>Day</th>
          <th>Start</th>
          <th>End</th>
          <th>Trainer</th>
          <th>Price</th>
          <th>Spaces Left</th>
        </tr>
        <% availableClasses.forEach(function(classes) { %>
        <tr>
          <td><%=classes.name %></td>
          <td><%=classes.day%></td>
          <td><%=classes.start%></td>
          <td><%=classes.end%></td>
          <td><%=classes.trainer%></td>
          <td>Â£<%=classes.price%></td>
          <td class="spaces"><%=classes.spaces%></td>
        </tr>
        <% }) %>
      </table>
    </div>    
    </body>
</html>
